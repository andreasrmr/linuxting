#installation
docker pull mysql

#kør en container.
#Bemærk storage kan  defineres.
docker run --name mysqldb -d -p 3306:3306 -e MYSQL_ROOT_PASSWORD=my-secret-pw mysql:latest

CONTAINER ID        IMAGE               COMMAND                  CREATED             STATUS              PORTS                               NAMES
a4c621e6482b        mysql:latest        "docker-entrypoint.s…"   4 seconds ago       Up 3 seconds        0.0.0.0:3306->3306/tcp, 33060/tcp   mysql1


#Enter docker exec mode
docker exec -it mysqldb /bin/bash

#tilgå server:
mysql -uroot -p123

#se ip adresse for container
docker inspect mysqldb

#check logs for container
docker logs mysqldb
